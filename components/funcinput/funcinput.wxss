@import "../../pages/houseprice/houseprice.wxss";
.houseprice-input-items{
    width: 100%;
    padding: 0rpx 5rpx;
}
.houseprice-body-item{
    position: relative;
    display: flex;
    align-items: center;
    width: 500rpx;
    height: 80rpx;
    line-height:80rpx;
    margin: 0rpx auto 0;
    background: #fff;
    border-radius: 16rpx;
}
.houseprice-item-iptbox {
    width: calc(100% - 20rpx);
    margin-left: 20rpx
}

.houseprice-item-iptbox input{
    width: 100%
}
.houseprice-input-item{
    width: calc( 100% - 25rpx)
}